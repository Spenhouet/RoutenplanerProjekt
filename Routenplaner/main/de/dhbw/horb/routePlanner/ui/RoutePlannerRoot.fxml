<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.dhbw.horb.routePlanner.ui.RoutePlannerRootController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="closeMenuButton" mnemonicParsing="false" onAction="#closeMenuButtonClicked" text="Close" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Kartendaten">
               <items>
                  <MenuItem fx:id="readDataButton" mnemonicParsing="false" onAction="#readDataButtonClicked" text="Daten einlesen" />
               </items>
            </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
